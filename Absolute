/* Description  :   C Program to find absolute value of an integer

 * Author       :   Keerthana G Pillai
 * Version      :   1.0
 * Date         :   28-01-2025
*/

#include<stdio.h>

int main()

{

   float num;

   printf("\n Enter number");

   scanf("\n %f",&num);

   if (num>0) {

    num=(-num);

    printf("\n The absolute value:%f",num);

   }

   return 0;

}
