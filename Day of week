/* Description  :   C Program to find which day of the week it is
 * Author       :   Keerthana G Pillai
 * Version      :   1.0
 * Date         :   29-01-2025
*/
#include<stdio.h>
int main()
{
    int day;
    printf("\n Enter the choice (1-7)");
    scanf("\n%d",&day);
    switch (day) {
        case 1:
         printf("\n Monday");
        
        case 2:
         printf("\n Tuesday");
         break;
        case 3:
         printf("\n Wednesday");
         break;
        case 4:
         printf("\n Thursday");
         break;
        case 5:
         printf("\n Friday");
         break;
        case 6:
         printf("\n Saturday");
         break;
        case 7:
         printf("\n Sunday");
         break;
    }
    return 0;
}
