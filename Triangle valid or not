
#include<stdio.h>

int main()

{

    int angle1,angle2,angle3, sum;

    printf(" Enter three angles");

    scanf("\n %d%d%d",&angle1,&angle2,&angle3);

    sum= angle1+angle2+angle3;

    if (sum==180) {

        printf("\nValid Triangle");

    }

    else{

        printf("\n Not a Valid Triangle");

    }

    return 0;

}
